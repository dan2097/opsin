    <suffix value="anilide">anilide</suffix>
    <suffix value="ate">ate</suffix>
    <suffix value="ic">ic</suffix>
    <suffix value="ium">ium</suffix>
    <suffix value="ol" subType="endInIne">ol</suffix>
    <suffix value="ol" subType="endInAn">ol</suffix>
    <suffix value="oyl">oyl</suffix>
    <suffix value="yl">oyl</suffix>
    <suffix value="oMeaningYl">yl</suffix><!--e.g. glycino-->
    <suffix value="aminoAcidYl">yl</suffix><!--locanted yl means something different to unlocanted yl!-->
  </groupType>

  <groupType type="chalcogenAcidStem"><!--e.g. sulfon to sulfonyl -->
    <suffix value="acylium">ylium</suffix>
    <suffix value="amide">amine</suffix>
    <suffix value="amidylium">aminylium</suffix>
    <suffix value="amido">amino</suffix>
