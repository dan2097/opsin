<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE suffixApplicability SYSTEM "suffixApplicability.dtd">
<!-- This file dictates what suffixes are applicabily to the group types that OPSIN recognises.
There are currently 3 main types: acidStem, substituent, standardGroup.
Other types are used under specific conditions e.g. chalcogenAcidStem, aminoAcid, mononuclearNonCarbonOxoAcid
If a type in OPSIN corresponds to a group type then it is used:
Hence acidStem is OPSIN type acidStem
All other groups are classified as standardGroup
In some cases it is further specified that a suffix only applies to a certain subtype e.g. yl has different meanings to different acids

The value of the suffix elements corresponds to the value attribute of the suffixes in suffixes.xml/inlineSuffixes.xml whilst the text is what should be looked up in suffixRules
-->
<suffixApplicability>
  <groupType type="substituent"><!--e.g. bor to boryl -->
    <suffix value="carbonyl">carbonyl</suffix>
    <suffix value="ide">ide</suffix>
    <suffix value="ium">ium</suffix>
    <suffix value="oxy">oxy</suffix>
    <suffix value="oyl">oyl</suffix>
    <suffix value="selenenyl">selenenyl</suffix>
    <suffix value="seleninyl">seleninyl</suffix>
    <suffix value="selenonyl">selenonyl</suffix>
    <suffix value="sulfenyl">sulfenyl</suffix>
    <suffix value="sulfinyl">sulfinyl</suffix>
    <suffix value="sulfonyl">sulfonyl</suffix>
    <suffix value="tellurenyl">tellurenyl</suffix>
    <suffix value="tellurinyl">tellurinyl</suffix>
    <suffix value="telluronyl">telluronyl</suffix>
    <suffix value="uide">uide</suffix>
    <suffix value="yl">yl</suffix>
    <suffix value="ylidene">ylidene</suffix>
    <suffix value="ylidyne">ylidyne</suffix>
    <suffix value="ylium">ylium</suffix>
  </groupType>

  <groupType type="acidStem"><!--e.g. acet to acetamide -->
    <suffix value="acylium">acylium</suffix>
    <suffix value="aldehyde">aldehyde</suffix>
    <suffix value="aldoxime">aldoxime</suffix>
    <suffix value="amide">amide</suffix>
    <suffix value="amidylium">amidylium</suffix>
    <suffix value="amidine">amidine</suffix>
    <suffix value="amidinium">amidinium</suffix>
    <suffix value="amidium">amidium</suffix>
    <suffix value="amido">amido</suffix>
    <suffix value="anilide">anilide</suffix>
    <suffix value="ate">ate</suffix>
    <suffix value="hydrazide">hydrazide</suffix>
    <suffix value="hydroxamate">hydroxamate</suffix>
    <suffix value="hydroxamic">hydroxamic</suffix>
    <suffix value="ic">ic</suffix>
    <suffix value="ide">ide</suffix>
    <suffix value="imide">cyclicimide</suffix>
    <suffix value="imidium">cyclicimidium</suffix>
    <suffix value="imido">cyclicimido</suffix>
    <suffix value="imidylium">cyclicimidylium</suffix>
    <suffix value="ium">ium</suffix>
    <suffix value="ite">ite</suffix>
    <suffix value="nitrile">nitrile</suffix>
    <suffix value="nitrilium">nitrilium</suffix>
    <suffix value="onaphthone">onaphthone</suffix>
    <!-- one is handled by hard coding common examples of its use -->
    <suffix value="ophenone">ophenone</suffix>
    <suffix value="ous">ous</suffix>
    <suffix value="oxy" subType="ylForAcyl">oxy-ylForAcyl</suffix>
    <suffix value="oxy" subType="ylForYl">oxy</suffix>
    <suffix value="oyl">oyl</suffix>
    <suffix value="uide">uide</suffix>
    <suffix value="yl" subType="ylForAcyl">oyl</suffix>
    <suffix value="yl" subType="ylForYl">yl</suffix>
    <suffix value="ylidene" subType="ylForYl">ylidene</suffix>
    <suffix value="ylidyne" subType="ylForYl">ylidyne</suffix>
    <suffix value="ylidene" subType="ylForAcyl">oxoAndDiYl</suffix>
    <suffix value="ylidyne" subType="ylForAcyl">oxoAndTriYl</suffix>
    <suffix value="ylium">ylium</suffix>
  </groupType>

  <groupType type="aminoAcid"><!--e.g. glycine -> glycinol -->
    <suffix value="al" subType="endInIne">aldehyde</suffix>
    <suffix value="al" subType="endInAn">aldehyde</suffix>
    <suffix value="aldehyde">aldehyde</suffix>
    <suffix value="aldoxime">aldoxime</suffix>
    <suffix value="amide">amide</suffix>
    <suffix value="amidylium">amidylium</suffix>
    <suffix value="amidium">amidium</suffix>
