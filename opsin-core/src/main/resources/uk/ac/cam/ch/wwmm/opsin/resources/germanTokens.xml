<!DOCTYPE tokenLists SYSTEM "tokenLists.dtd">
<!-- This file contains some of the most common pieces of vocab that differ in German from English
It is far from comprehensive -->
<tokenLists>
	<tokenList tagname="group" type="substituent" subType="halideOrPseudoHalide" symbol="x">
		<token value="-Br" labels="none">brom</token>
		<token value="-Cl" labels="none">chlor</token>
		<token value="-F" labels="none">fluor</token>
		<token value="-I" labels="none">iod</token>
	</tokenList>

	<tokenList tagname="group" type="substituent" subType="perhalogeno" symbol="x">
		<token value="-F" labels="none">perfluor</token>
		<token value="-Br" labels="none">perbrom</token>
		<token value="-Cl" labels="none">perchlor</token>
		<token value="-I" labels="none">period</token>
	</tokenList>
</tokenLists>
