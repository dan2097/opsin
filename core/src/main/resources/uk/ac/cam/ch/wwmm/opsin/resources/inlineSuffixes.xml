<!DOCTYPE tokenLists SYSTEM "tokenLists.dtd">
<!-- See the DTD for more details -->
<!-- value = a reference to suffixRules.xml -->
<tokenLists>
  <tokenList tagname="suffix" type="inline" symbol="i">
    <token value="amido">amido</token>
    <token value="oyl" additionalValue="amide">amoyl</token>
    <token value="carbohydrazonoyl">carbohydrazonoyl</token>
    <token value="carbohydroximoyl">carbohydroximoyl</token>
    <token value="carbonyl">carbonyl</token>
    <token value="carbothioyl">carbothioyl</token>
    <token value="carboxamido">carboxamido</token>
    <token value="carboximidoyl">carboximidoyl</token>
    <token value="hydrazonoyl">hydrazonoyl</token>
    <token value="hydroximoyl">hydroximoyl</token>
    <token value="amido" additionalValue="aldehyde">imido</token><!--suffixes joined by PreStructureBuilder-->
    <token value="imidoyl">imidoyl</token>
    <token value="hydrazonoyl">ohydrazonoyl</token>
    <token value="hydroximoyl">ohydroximoyl</token>
    <token value="oxy">oxy</token>
    <token value="oyl">oyl</token>
    <token value="sulfenyl">sulfenyl</token>
    <token value="sulfinyl">sulfinyl</token>
    <token value="sulfonyl">sulfonyl</token>
    <token value="yl">yl</token>
    <token value="ylidene">ylidene</token>
    <token value="ylidyne">ylidyne</token>
  </tokenList>

  <tokenList tagname="suffix" type="inline" symbol="w"><!--are allowed after another inline suffix, only the basic inline suffixes etc.-->
    <token value="amido">amido</token>
    <token value="oyl">oyl</token>
    <token value="yl">yl</token>
    <token value="ylidene">ylidene</token>
    <token value="ylidyne">ylidyne</token>
  </tokenList>
</tokenLists>