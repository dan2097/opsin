    <suffix value="hydroxamic">hydroxamic</suffix>
    <suffix value="hydroximate">hydroximate</suffix>
    <suffix value="hydroximic">hydroximic</suffix>
    <suffix value="hydroximoyl">hydroximoyl</suffix>
    <suffix value="ic">ic</suffix>
    <suffix value="ide">ide</suffix>
    <suffix value="imine">imine</suffix>
    <suffix value="iminide">iminide</suffix>
    <suffix value="iminium">iminium</suffix>
    <suffix value="io">io</suffix>
