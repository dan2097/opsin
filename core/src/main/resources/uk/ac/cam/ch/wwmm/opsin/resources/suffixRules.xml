<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE suffixRulesList SYSTEM "suffixRules.dtd">
<!-- See the DTD and the OPSIN code (PreStructureBuilder.resolveGroupAddingSuffixes and PreStructureBuilder.resolveSuffixes) to make
sense of this -->
<suffixRulesList>
  <rule value="aldehyde">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="O" bondOrder="2" />
  </rule>
  <rule value="amide">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="O" bondOrder="2" />
    <addgroup SMILES="N" functionalIDs="1,1" />
  </rule>
  <rule value="amidine">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="N" functionalIDs="1,1"/>
    <addgroup SMILES="N" bondOrder="2" />
  </rule>
  <rule value="amidium">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="O" bondOrder="2" />
    <addgroup SMILES="[N+]" functionalIDs="1,1" />
  </rule>
  <rule value="amido">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="O" bondOrder="2" />
    <addgroup SMILES="N" setsOutID="yes" />
  </rule>
  <rule value="amine">
    <addgroup SMILES="N" />
  </rule>
  <rule value="aminide">
    <addgroup SMILES="[N-]" />
  </rule>
  <rule value="aminium">
    <addgroup SMILES="[N+]" />
  </rule>
  <rule value="amino">
    <addgroup SMILES="N" setsOutID="yes"/>
  </rule>
  <rule value="anilide">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="O" bondOrder="2" />
    <addgroup SMILES="Nc1ccccc1" labels="/1'/2'/3'/4'/5'/6'"/>
  </rule>
  <rule value="ate">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="O" bondOrder="2" />
    <addgroup SMILES="[O-]" functionalIDs="1" />
  </rule>
  <rule value="ate_nonCarboxylic">
    <addFunctionalIDsToHydroxyGroups />
    <chargeHydroxyGroups />
  </rule>
  <rule value="boronic">
    <addgroup SMILES="B(O)O" functionalIDs="3"/>
  </rule>
  <rule value="carbonyl">
    <addgroup SMILES="C(=O)" setsOutID="yes"/>
  </rule>
  <rule value="carboxylic">
    <addgroup SMILES="C(=O)O" functionalIDs="3"/>
  </rule>
  <rule value="carboxylate">
    <addgroup SMILES="C(=O)[O-]" functionalIDs="3" />
  </rule>
  <rule value="carboxylite">
    <addgroup SMILES="C[O-]" functionalIDs="2" />
  </rule>
  <rule value="diazonium">
    <addgroup SMILES="[N+]#N" />
  </rule>
  <rule value="hydrazide">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="O" bondOrder="2" />
    <addgroup SMILES="NN" labels="1',N/2',N'"/>
  </rule>
  <rule value="hydrazonic">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="NN" bondOrder="2" />
    <addgroup SMILES="O" functionalIDs="1"/>
  </rule>
  <rule value="hydroxamate">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="N[O-]" functionalIDs="2"/>
    <addgroup SMILES="O" bondOrder="2" />
  </rule>
  <rule value="hydroxamic">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="NO" functionalIDs="2"/>
    <addgroup SMILES="O" bondOrder="2" />
  </rule>
  <rule value="hydroximate">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="NO" bondOrder="2" />
    <addgroup SMILES="[O-]" functionalIDs="1"/>
  </rule>
  <rule value="hydroximic">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="NO" bondOrder="2" />
    <addgroup SMILES="O" functionalIDs="1"/>
  </rule>
  <rule value="hydroximoyl">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="NO" bondOrder="2"/>
    <setOutID />
  </rule>
  <rule value="ic">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="O" functionalIDs="1"/>
    <addgroup SMILES="O" bondOrder="2" />
  </rule>
  <rule value="ic_nonCarboxylic">
    <addFunctionalIDsToHydroxyGroups/>
  </rule>
  <rule value="ide">
    <changecharge charge="-1" protons="-1" />
  </rule>
  <rule value="imine">
    <addgroup SMILES="N" bondOrder="2" />
  </rule>
  <rule value="iminide">
    <addgroup SMILES="[N-]" bondOrder="2" />
  </rule>
  <rule value="iminium">
    <addgroup SMILES="[N+]" bondOrder="2" />
  </rule>
  <rule value="io">
    <setOutID />
    <changecharge charge="1" protons="1"/>
  </rule>
  <rule value="ite">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="[O-]" functionalIDs="1" />
  </rule>
  <rule value="ite_nonCarboxylic">
    <removeOneDoubleBondedOxygen />
    <addFunctionalIDsToHydroxyGroups />
    <chargeHydroxyGroups />
  </rule>
  <rule value="ium">
    <changecharge charge="1" protons="1"/>
  </rule>
  <rule value="nitrile">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="N" bondOrder="3" />
  </rule>
  <rule value="nitrilium">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="[N+]" bondOrder="3" />
  </rule>
  <rule value="ol">
    <addgroup SMILES="O" />
  </rule>
  <rule value="olate">
    <addgroup SMILES="[O-]" />
  </rule>
  <rule value="onaphthone">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="O" bondOrder="2" />
    <addgroup SMILES="c1cccc2ccccc12" labels="1'/2'/3'/4'/4a'/5'/6'/7'/8'/8a'" />
  </rule>
  <rule value="one">
    <addgroup SMILES="O" bondOrder="2" ketoneLocant="yes" />
  </rule>
  <rule value="ophenone">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="O" bondOrder="2" />
    <addgroup SMILES="c1ccccc1" labels="1'/2'/3'/4'/5'/6'" />
  </rule>
  <rule value="ous">
    <addgroup SMILES="O" functionalIDs="1"/>
  </rule>
  <rule value="ous_nonCarboxylic">
    <removeOneDoubleBondedOxygen />
    <addFunctionalIDsToHydroxyGroups/>
  </rule>
  <rule value="oxoAndDiYl">
    <addgroup SMILES="O" bondOrder="2" />
    <setOutID />
    <setOutID />
  </rule>
  <rule value="oxoAndTriYl">
    <addgroup SMILES="O" bondOrder="2" />
    <setOutID />
    <setOutID />
    <setOutID />
  </rule>
  <rule value="oxy">
    <addgroup SMILES="O" setsOutID="yes" />
  </rule>
  <rule value="oxy-ylForAcyl">
    <addgroup SMILES="O" bondOrder="2" />
    <addgroup SMILES="O" setsOutID="yes" />
  </rule>
  <rule value="oyl">
    <addSuffixPrefixIfNonePresentAndCyclic SMILES="C"/>
    <addgroup SMILES="O" bondOrder="2" />
    <setOutID />
  </rule>
  <rule value="oyl_nonCarboxylic">
    <convertHydroxyGroupsToOutIDs />
  </rule>
  <rule value="selenenic">
    <addgroup SMILES="[Se]O" functionalIDs="2"/>
  </rule>
  <rule value="selenenyl">
    <addgroup SMILES="[Se]" setsOutID="yes"/>
  </rule>
  <rule value="seleninic">
    <addgroup SMILES="[Se](=O)O" functionalIDs="3"/>
  </rule>
  <rule value="seleninyl">
    <addgroup SMILES="[Se](=O)" setsOutID="yes"/>
  </rule>
  <rule value="selenonic">
    <addgroup SMILES="[Se](=O)(=O)O" functionalIDs="4"/>
  </rule>
  <rule value="selenonyl">
    <addgroup SMILES="[Se](=O)(=O)" setsOutID="yes"/>
  </rule>
  <rule value="selone">
    <addgroup SMILES="[Se]" bondOrder="2" ketoneLocant="yes" />
  </rule>
  <rule value="sulfenamide">
    <addgroup SMILES="SN" functionalIDs="2"/>
  </rule>
  <rule value="sulfenamido">
    <addgroup SMILES="SN-" setsOutID="yes"/>
  </rule>
  <rule value="sulfenic">
    <addgroup SMILES="SO" functionalIDs="2"/>
  </rule>
  <rule value="sulfenoselenoic">
    <addgroup SMILES="S[Se]" functionalIDs="2"/>
  </rule>
  <rule value="sulfenoselenoyl">
    <addgroup SMILES="S[Se]-" setsOutID="yes"/>
  </rule>
  <rule value="sulfenothioic">
    <addgroup SMILES="SS" functionalIDs="2"/>
  </rule>
  <rule value="sulfenothioyl">
    <addgroup SMILES="SS-" setsOutID="yes"/>
  </rule>
  <rule value="sulfenyl">
    <addgroup SMILES="S" setsOutID="yes"/>
  </rule>
  <rule value="sulfinic">
    <addgroup SMILES="S(=O)O" functionalIDs="3"/>
  </rule>
  <rule value="sulfinyl">
    <addgroup SMILES="S(=O)" setsOutID="yes"/>
  </rule>
  <rule value="sulfonic">
    <addgroup SMILES="S(=O)(=O)O" functionalIDs="4"/>
  </rule>
  <rule value="sulfonyl">
    <addgroup SMILES="S(=O)(=O)" setsOutID="yes"/>
  </rule>
  <rule value="tellone">
    <addgroup SMILES="[Te]" bondOrder="2" ketoneLocant="yes" />
  </rule>
  <rule value="tellurenic">
    <addgroup SMILES="[Te]O" functionalIDs="2"/>
  </rule>
  <rule value="tellurenyl">
    <addgroup SMILES="[Te]" setsOutID="yes"/>
  </rule>
  <rule value="tellurinic">
    <addgroup SMILES="[Te](=O)O" functionalIDs="3"/>
  </rule>
  <rule value="tellurinyl">
    <addgroup SMILES="[Te](=O)" setsOutID="yes"/>
  </rule>
  <rule value="telluronic">
    <addgroup SMILES="[Te](=O)(=O)O" functionalIDs="4"/>
  </rule>
  <rule value="telluronyl">
    <addgroup SMILES="[Te](=O)(=O)" setsOutID="yes"/>
  </rule>
  <rule value="thiolate">
    <addgroup SMILES="[S-]" />
  </rule>
  <rule value="uide">
    <changecharge charge="-1" protons="1" />
  </rule>
  <rule value="yl">
    <setOutID />
  </rule>
  <rule value="diyl">
    <setOutID />
    <setOutID />
  </rule>
  <rule value="ylidene">
    <setOutID outValency="2" />
  </rule>
  <rule value="ylidyne">
    <setOutID outValency="3" />
  </rule>
  <rule value="ylium">
    <changecharge charge="1" protons="-1" />
  </rule>
</suffixRulesList>