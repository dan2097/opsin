<!DOCTYPE tokenLists SYSTEM "tokenLists.dtd">
<!-- See the DTD for more details -->
<!-- value = a reference to suffixApplicability.xml -->
<!--Suffix subType is used to indicate those that occur at the end of chains.
See the des-->
<tokenLists>
<!--not allowed to be prefixed by carbo(x)/sulfon etc. Can be locanted to any position-->
  <tokenList tagname="suffix" type="root" symbol="s">
    <token value="amine">amine</token>
    <token value="aminide">aminide</token>
    <token value="aminium">aminium</token>
    <token value="carbaldehyde">carbaldehyde</token>
    <token value="carbohydroxamic">carbohydroxamic</token>
    <token value="carbohydroximic">carbohydroximic</token>
    <token value="carboxylate">carboxylate</token>
    <token value="carboxylic">carboxylic</token>
    <token value="carboxylite">carboxylite</token>
    <token value="diazonium">diazonium</token>
    <token value="imine">imine</token>
    <token value="iminide">iminide</token>
    <token value="iminium">iminium</token>
    <token value="ol">ol</token>
    <token value="olate">olate</token>
    <token value="one">one</token>
    <token value="selone">selone</token>
    <token value="selenenic">selenenic</token>
    <token value="sulfenoselenoic">sulfenoselenoic</token>
    <token value="sulfenothioic">sulfenothioic</token>
    <token value="sulfenamide">sulfenamide</token>
    <token value="sulfenic">sulfenic</token>
    <token value="tellone">tellone</token>
    <token value="tellurenic">tellurenic</token>
    <token value="thiolate">thiolate</token>

<!--common acids-this method is no longer recommended-->
    <token value="arsonic">arsonic</token>
    <token value="arsonic">arsoric</token>
    <token value="boronic">boronic</token>
    <token value="carbamic">carbamic</token>
    <token value="phosphonic">phosphonic</token>

    <token value="arsonate">arsonate</token>
    <token value="arsorate">arsorate</token>
    <token value="boronate">boronate</token>
    <token value="carbamate">carbamate</token>
    <token value="phosphonate">phosphonate</token>
  </tokenList>

<!--not allowed to prefixed by carbo(x)/sulfon etc. Implicitly must be at a terminal position-->
  <tokenList tagname="suffix" type="root" subType="terminal" symbol="s">
    <token value="aldehyde">al</token>
    <token value="ic" additionalValue="aldehyde">aldehydic</token>
    <token value="ate" additionalValue="amide">amate</token>
    <token value="ic" additionalValue="amide">amic</token>
    <token value="ic" additionalValue="anilide">anilic</token>
    <token value="amide" additionalValue="aldehyde">imide</token><!--suffixes joined by PreStructureBuilder-->
    <token value="amidium" additionalValue="aldehyde">imidium</token><!--suffixes joined by PreStructureBuilder-->
    <token value="ite">ite</token>
    <token value="onaphthone">onaphthone</token>
    <token value="ophenone">ophenone</token>
    <token value="ous">ous</token>
  </tokenList>

<!--allowed to prefixed by carbo(x)/sulfon etc. e.g. carboxamide, sulfonamide etc.-->
<!--these suffixes may have a euphonic o in front of them-->
  <tokenList tagname="suffix" type="root" subType="terminal" symbol="å">
    <token value="aldehyde">aldehyde</token>
    <token value="amide">amide</token>
    <token value="amidium">amidium</token>
    <token value="anilide">anilide</token>
    <token value="amidine">amidine</token><!--deprecated-->
    <token value="ate">ate</token>
    <token value="hydrazide">hydrazide</token>
    <token value="hydroxamic">hydroxamic</token>
    <token value="hydroximic">hydroximic</token>
    <token value="hydroxamate">hydroxamate</token>
    <token value="hydroximate">hydroximate</token>
    <token value="ic">ic</token>
    <token value="nitrile">nitrile</token>
    <token value="nitrilium">nitrilium</token>
  </tokenList>
</tokenLists>